page {
  background-color: #f5f5f5;
}
.canvas_box {
  position: relative;
}
.share_canvas {
  position: fixed;
  /* left: 50%; */
  left: -200%;
  /* top: -50%; */
  /* z-index: 9; */
  /* transform: translate(-50%, -50%); */
}
.team_detail {
  padding: 42rpx 24rpx 48rpx 24rpx;
  margin-bottom: 72rpx;
  background-color: #fff;
}

.good_content {
  width: 662rpx;
  height: 240rpx;
  padding: 20rpx;
  margin-bottom: 24rpx;
  background-color:#ffffff;
  border-radius: 16rpx;
  box-shadow: 0rpx  4rpx  8rpx rgba(0, 0, 0, 0.1);
}

.cover {
  position: relative;
  flex-grow: 0;
  flex-shrink: 0;
  width: 240rpx;
  height: 240rpx;
  margin-right: 24rpx;
  border-radius: 16rpx;
  overflow: hidden;
}
.new_team {
  position: absolute;
  top: 0;
  left: 0;
  width: 48rpx;
  height: 86rpx;
}

.title {
  height: 84rpx;
  color: #333333;
  font-size: 32rpx;
  line-height: 42rpx;
  margin-bottom: 24rpx;
}

.desc {
  height: 30rpx;
  color: #999999;
  font-size: 24rpx;
  line-height: 30rpx;
  /* margin-bottom: 42rpx; */
}

.price_text {
  color: #e7532c;
  font-size: 28rpx;
  line-height: 40rpx;
  margin-right: 6rpx;
}


.time_text {
  font-weight: 600;
  color: #333333;
  font-size: 36rpx;
  line-height: 50rpx;
  margin-right: 12rpx;
}

.text_center {
  text-align: center;
}

.time_end {
  width: 620rpx;
  color: #666666;
  font-size: 32rpx;
  line-height: 44rpx;
  text-align: center;
  margin: 0 auto;
}

.red {
  color: #e7532c;
}

.btn {
  width: 702rpx;
  height: 96rpx;
  color: #ffffff;
  font-size: 32rpx;
  line-height: 96rpx;
  text-align: center;
  margin: 0 auto;
  background-color: #FF0000;
  border-radius: 48rpx;
}

.rule {
  padding-top: 12rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 28rpx;
  color: #999999;
}
.one {
  display: flex;
  justify-content: flex-start;
}
.t2 {
  color: #666666;
  margin-left: 24rpx;
}

.rule_icon {
  width: 24rpx;
  height: 24rpx;
  overflow: hidden;
}


.goods_tag {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 16rpx;
  width: 200rpx;
  height: 34rpx;
  background: #FFECEC;
  border-radius: 20rpx;
}

.goods_tag_icon {
  display: inline-block;
  margin-right: 4rpx;
  width: 30rpx;
  height: 30rpx;
}

.goods_tag_test {
  display: inline-block;
  font-size: 24rpx;
  font-weight: 400;
  color: #FF0000;
  line-height: 34rpx;
}


.list-title {
  display: flex;
  justify-content: center;
  align-items: center;
}
.title-icon {
  margin-right: 8rpx;
  width: 38rpx;
  height: 38rpx;
}
.hot_head_line {
  width: 96rpx;
  height: 2rpx;
}

.left_line {
  margin-right: 8rpx;
  background: linear-gradient(-90deg, #ccc 0%, rgba(204, 204, 204, 0) 100%);
}

.right_line {
  margin-left: 8rpx;
  background: linear-gradient(90deg, #ccc 0%, rgba(204, 204, 204, 0) 100%);
}

.list-box {
  padding: 24rpx;
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}
.list-item {
  display: flex;
  flex-direction: column;
  width: 344rpx;
  height: 524rpx;
  background: #FFFFFF;
  border-radius: 16rpx;
  margin-bottom: 16rpx;
  overflow: hidden;
}
.list-item:nth-child(even) {
  margin-left: 14rpx;
}
.list-img {
  width: 100%;
  height: 344rpx;
  overflow: hidden;
}
.list-item-name {
  margin-top: 16rpx;
  margin-bottom: 8rpx;
  padding: 0 16rpx;
  height: 40rpx;
  font-size: 28rpx;
  color: #333333;
  line-height: 40rpx;
}
.list-item-ed {
  padding: 0 16rpx;
  margin-bottom: 20rpx;
  height: 30rpx;
  font-size: 24rpx;
  color: #999999;
  line-height: 30rpx;
}

.list-item-price {
  display: flex;
  justify-content: flex-start;
  align-items: flex-end;
  
}
.p-text {
  padding-left: 18rpx;
  margin-right: 8rpx;
  font-size: 28rpx;
  color: #FF0000;
  vertical-align:bottom;
}
.price1 {
  font-size: 24rpx;
  color: #FF0000;
  vertical-align:bottom;
}
.price2 {
  position: relative;
  top: 4rpx;
  margin-right: 8rpx;
  font-size: 36rpx;
  color: #FF0000;
  vertical-align:bottom;
}
.price3 {
  position: relative;
  top: -2rpx;
  font-size: 20rpx;
  color: #B2B2B2;
  text-decoration: line-through;
  vertical-align:bottom;
}


.null-box {
  margin-bottom: 48rpx;
  width: 100%;
  height: 640rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  background-color: #FFFFFF;
}

.null-img {
  margin-bottom: 16rpx;
  width: 320rpx;
  height: 320rpx;
  overflow: hidden;
}
.null-text {
  width: 434rpx;
  height: 40rpx;
  font-size: 28rpx;
  color: #999999;
  line-height: 40px;
  margin-bottom: 64rpx;
}
.null-btn {
  width: 236rpx;
  height: 72rpx;
  border-radius: 36rpx;
  border: 1rpx solid #FF0000;
  line-height: 72rpx;
  text-align: center;
  font-size: 28rpx;
  color: #FF0000;
}


.share_btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 702rpx;
  height: 96rpx;
  color: #ffffff;
  font-size: 32rpx;
  line-height: 96rpx;
  text-align: center;
  margin: 0 auto;
  background-color: #FF0000;
  border-radius: 48rpx;
}
