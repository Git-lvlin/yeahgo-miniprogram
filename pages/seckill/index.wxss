
.back {
  position: fixed;
  left: 46rpx;
  width: 48rpx;
  height: 48rpx;
  z-index: 2;
}

.container {
  position: relative;
  z-index: 1;
  display: flex;
  flex-direction: column;
  height: 100vh;
  /* overflow: hidden; */
}

.banner {
  width: 100%;
  height: 360rpx;
}

.swiper {
  width: 124rpx;
  height: 100%;
  box-sizing: border-box;
  margin-right: 66rpx;
}
.swiper-item {
  display: flex;
  align-items: center;
  transform: 1s;
}
.visible {
  overflow: visible;
}
.op {
  opacity: 0;
}
.userIcon {
  width: 42rpx;
  height: 42rpx;
  border: 1px solid #FFFFFF;
  border-radius: 50%;
}

.top-box {
  position: absolute;
  top: 240rpx;
  left: 50%;
  transform: translate(-50%);
  width: 704rpx;
  height: 58rpx;
  background-color: rgba(154,22,19, .62);
  border-radius: 28rpx;
  z-index: 2;
  display: flex;
  justify-content: center;
  align-items: center;
}
.top-number {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
  margin-right: 12rpx;
}
.time {
  display: inline-block;
  width: 36rpx;
  height: 36rpx;
  background: #FFFFFF;
  border-radius: 6rpx;
  line-height: 36rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #9A1613;
  text-align: center;
}
.time-box {
  display: flex;
  align-items: center;
}
.text {
  margin-right: 12rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}
.time-symbol {
  color: #FFFFFF;
  margin: 0 6rpx;
  font-size: 22rpx;
}

.tab-box {
  position: relative;
  top: -46rpx;
  width: 100%;
  height: 100rpx;
}
.van-tabs__scroll {
  border-radius: 24rpx 24rpx 0 0 !important;
  background-color: none !important;
  overflow: hidden !important;
}
.nav-class {
  background-color: #FFECEC !important;
  border-radius: 24rpx 24rpx 0 0 !important;
}
.van-tab--active {
  border-radius: 24rpx 24rpx 0 0 !important;
  box-shadow: 0px 0px 20rpx rgba(255,51,51, .6) !important;
}

.tab-active .van-ellipsis {
  transform:scale(1.2) !important;
  color: #FF3333 !important;
  background: #FFFFFF !important;
  border-radius: 24rpx 24rpx 0 0 !important;
}
.custom .van-tabs__line {
  height: 6rpx;
  background: linear-gradient(90deg, #F20825 0%, #FEB900 100%) !important;
  border-radius: 2rpx !important;
}

.today,.tomorrow {
  padding: 40rpx;
}
.tab-item {
  position: relative;
  width: 100%;
  height: 234rpx;
  display: flex;
  justify-content: center;
  margin-bottom: 32rpx;
}
.left-img {
  width: 234rpx;
  height: 234rpx;
  border-radius: 16rpx;
  overflow: hidden;
}
.right-box {
  margin-left: 16rpx;
  width: 436rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
}
.title {
  width: 100%;
  height: 44rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  line-height: 44rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 8rpx;
}
.progress-bar-box {
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.progress-bar {
  display: flex;
  align-items: center;
  width: 290rpx;
  height: 28rpx;
  background: #FBE9E6;
  border-radius: 14rpx;
  display: flex;
  justify-content: flex-start;
}
.progress-index {
  border-radius: 14rpx;
  height: 100%;
  line-height: 28rpx;
  text-align: center;
  background: linear-gradient(90deg, #FE2C20 0%, #FF463C 100%);
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #FFFFFF;
}
.progress-img {
  position: relative;
  left: -20rpx;
