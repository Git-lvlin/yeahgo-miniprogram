      {
        "pagePath": "pages/user/index",
        "text": "我的",
        "iconPath": "/images/tabbar/me@3x.png",
        "selectedIconPath": "/images/tabbar/actMe@3x.png"
      }
    ]
  },
  "usingComponents": {
    "van-image": "/miniprogram_npm/@vant/weapp/image/index"
